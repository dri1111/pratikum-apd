<?xml version="1.0"?>
<flowgorithm fileversion="4.2">
    <attributes>
        <attribute name="name" value="diskon_laptop_loop"/>
        <attribute name="authors" value=""/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2025-09-23 01:31:56 PM"/>
        <attribute name="created" value="d2VsY29tZTtEUklYWjsyMDI1LTA5LTIzOzAxOjIwOjEzIFBNOzI0MjM="/>
        <attribute name="edited" value="d2VsY29tZTtEUklYWjsyMDI1LTA5LTIzOzAxOjMxOjU2IFBNOzE7MjU0MA=="/>
    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="nama" type="String" array="False" size=""/>
            <declare name="nim" type="String" array="False" size=""/>
            <declare name="harga" type="Real" array="False" size=""/>
            <declare name="merek" type="String" array="True" size="3"/>
            <declare name="diskonPersen" type="Integer" array="True" size="3"/>
            <declare name="diskonNominal" type="Real" array="False" size=""/>
            <declare name="hargaAkhir" type="Real" array="False" size=""/>
            <declare name="i" type="Integer" array="False" size=""/>
            <output expression="&quot;Masukan Nama&quot;" newline="False"/>
            <input variable="nama"/>
            <output expression="&quot;Masukan Nim&quot;" newline="False"/>
            <input variable="nim"/>
            <output expression="&quot;Masukan Harga&quot;" newline="False"/>
            <input variable="harga"/>
            <assign variable="merek[0]" expression="&quot;Acer&quot;"/>
            <assign variable="merek[1]" expression="&quot;Asus&quot;"/>
            <assign variable="merek[2]" expression="&quot;Lenovo&quot;"/>
            <assign variable="diskonPersen[0]" expression="5"/>
            <assign variable="diskonPersen[1]" expression="7"/>
            <assign variable="diskonPersen[2]" expression="10"/>
            <output expression="nama &amp; &quot; dengan NIM &quot; &amp; nim &amp; &quot; ingin membeli laptop seharga Rp &quot; &amp; harga" newline="True"/>
            <output expression="&quot;Berikut harga laptop setelah diskon tiap merek:&quot;" newline="True"/>
            <output expression="&quot;Merek\t Diskon\t Harga Akhir&quot;" newline="True"/>
            <for variable="i" start="0" end="2" direction="inc" step="1">
                <assign variable="diskonNominal" expression="harga * diskonPersen[i] / 100"/>
                <assign variable="hargaAkhir" expression="harga - diskonNominal"/>
                <output expression="merek[i] &amp; &quot;    &quot; &amp; diskonPersen[i] &amp; &quot;%    Rp&quot; &amp; hargaAkhir" newline="True"/>
            </for>
        </body>
    </function>
</flowgorithm>
