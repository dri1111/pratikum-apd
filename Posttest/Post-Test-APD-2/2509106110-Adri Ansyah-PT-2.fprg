<?xml version="1.0"?>
<flowgorithm fileversion="4.2">
    <attributes>
        <attribute name="name" value="diskon_laptop_identitas_fixed"/>
        <attribute name="authors" value=""/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2025-09-23 07:43:00 AM"/>
        <attribute name="created" value="d2VsY29tZTtEUklYWjsyMDI1LTA5LTIzOzEyOjU3OjM5IEFNOzI0Mjg="/>
        <attribute name="edited" value="d2VsY29tZTtEUklYWjsyMDI1LTA5LTIzOzA3OjQzOjAwIEFNOzI7MjUyNA=="/>
    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="nim" type="String" array="False" size=""/>
            <declare name="nama" type="String" array="False" size=""/>
            <declare name="harga" type="Integer" array="False" size=""/>
            <declare name="pilihan" type="Integer" array="False" size=""/>
            <declare name="merek" type="String" array="False" size=""/>
            <declare name="persendiskon" type="Integer" array="False" size=""/>
            <declare name="diskon" type="Real" array="False" size=""/>
            <declare name="hargaakhir" type="Real" array="False" size=""/>
            <output expression="&quot;Masukan Nim&quot;" newline="False"/>
            <input variable="nim"/>
            <output expression="&quot;Masukan Nama&quot;" newline="False"/>
            <input variable="nama"/>
            <output expression="&quot;Masukan Harga&quot;" newline="False"/>
            <input variable="harga"/>
            <output expression="&quot;pilih merek laptop&quot;" newline="True"/>
            <output expression="&quot;1&quot;" newline="True"/>
            <output expression="&quot;2&quot;" newline="True"/>
            <output expression="&quot;3&quot;" newline="True"/>
            <input variable="pilihan"/>
            <if expression="pilihan = 1">
                <then>
                    <assign variable="merek" expression="&quot;Acer&quot;"/>
                    <assign variable="persendiskon" expression="5"/>
                </then>
                <else>
                    <if expression="pilihan = 2">
                        <then>
                            <assign variable="merek" expression="&quot;Asus&quot;"/>
                            <assign variable="persendiskon" expression="7"/>
                        </then>
                        <else>
                            <if expression="pilihan = 3">
                                <then>
                                    <assign variable="merek" expression="&quot;Lenovo&quot;"/>
                                    <assign variable="persendiskon" expression="10"/>
                                </then>
                                <else>
                                    <assign variable="merek" expression="&quot;Tidak Valid&quot;"/>
                                    <assign variable="persendiskon" expression="0"/>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
            <assign variable="diskon" expression="harga * persendiskon / 100"/>
            <assign variable="hargaakhir" expression="harga - diskon"/>
            <output expression="&quot;===== STRUK PEMBELIAN =====&quot;" newline="True"/>
            <output expression="&quot;NIM : &quot; &amp; nim" newline="True"/>
            <output expression="&quot;Nama : &quot; &amp; nama" newline="True"/>
            <output expression="&quot;Merek Laptop : &quot; &amp; merek" newline="True"/>
            <output expression="&quot;Harga Awal : Rp &quot; &amp; harga" newline="True"/>
            <output expression="&quot;Diskon (%) : &quot; &amp; persendiskon" newline="True"/>
            <output expression="&quot;Jumlah Diskon : Rp &quot; &amp; diskon" newline="True"/>
            <output expression="&quot;Harga Akhir : Rp &quot; &amp; hargaakhir" newline="True"/>
            <output expression="&quot;============================&quot;" newline="True"/>
        </body>
    </function>
</flowgorithm>
